<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.omo.projectomo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/omo_logo_64x64"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectOMO">
        <activity
            android:name=".Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckStolen" android:label="check"></activity>
        <activity android:name=".ClaimStolen" android:label="claim"></activity>
        <activity android:name=".ClaimFound" android:label="delete"></activity>
        <activity android:name=".MainActivity" android:label="main"></activity>
        <activity android:name=".MessageBoard" android:label="message"></activity>
    </application>

</manifest>